{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "config schema json",
  "description": "json validator",
  "type": "object",
  "properties": {
    "logging": {
      "type": "object",
      "description": "Logging Resource",
      "properties": {
        "loglevel": {
          "enum": [
            "INFO",
            "DEBUG",
            "WARN",
            "ERROR",
            "FATAL",
            "OFF",
            "TRACE",
            "ALL"
          ],
          "type": "string",
          "description": "Sets the log level of the microservice."
        }
      },
      "required": [
        "loglevel"
      ]
    }
  }
}
